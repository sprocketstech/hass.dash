{
    "name": "Calendar",
    "module": "hassdash.widget.calendar",
    "controller": "calendarController",
    "editController": "calendarEditController",
    "configController": "calendarConfigController",
    "show_label": false,
    "num_entities": "many",
    "entity_filter": [
        "calendar.*"
    ],
    "availableSizes": [
        {"name": "M", "value": {"x": 5, "y": 6}},
        {"name": "L", "value": {"x": 10, "y": 10}},
        {"name": "XL", "value": {"x": 15, "y": 15}},
        {"name": "XXL", "value": {"x": 25, "y": 15}}
    ],
    "options": [
    ],
    "templateUrl": "calendar/calendar.html",
    "templateCss": ["calendar/fullcalendar.min.css",
                    "calendar/calendar.css"],
    "templateJs": ["calendar/fullcalendar.min.js",
                    "calendar/ui-calendar.js", 
                    "calendar/calendar.js"],
    "configUrl": "calendar/configCalendar.html"
}